{
  "name": "wasm-qr-scanner",
  "version": "0.1.0",
  "description": "High-performance QR code scanner for the web, powered by ZXing-C++ WebAssembly",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./dist/worker.js": "./dist/worker.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "dev": "cd demo && vite",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "qr",
    "qr-code",
    "scanner",
    "barcode",
    "wasm",
    "webassembly",
    "zxing",
    "camera"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "zxing-wasm": "^2.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/qrcode": "^1.5.6",
    "jsdom": "^25.0.0",
    "qrcode": "^1.5.4",
    "sharp": "^0.33.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
